package repleceofnumber;

public class revstring {
	public static void main(String[] args) {
		 String s="level";
		 String rev="";
		 for(int i=s.length();i<=0;i--)
		 {
			 rev+=s.charAt(i);
			 
	
			 
		 }
		 System.out.println(rev);
		 System.out.println( rev.equals(s)?"padio":"not pas");

 	}
 

}
